{
  "name": "cypress-debugger",
  "version": "1.0.7",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "author": "Currents Software Inc",
  "homepage": "https://github.com/currents-dev/cypress-debugger",
  "bugs": {
    "url": "https://github.com/currents-dev/cypress-debugger/issues",
    "email": "support@currents.dev"
  },
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=14.7.0"
  },
  "files": [
    "dist",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsup-node --dts",
    "dev": "tsup --watch",
    "lint": "eslint --fix",
    "publish": "node ./publish.js"
  },
  "keywords": [
    "cypress",
    "e2e",
    "cypress-debugger",
    "currents",
    "cypress-replay",
    "cypress-traces",
    "replay",
    "sorry-cypress",
    "time-travel"
  ],
  "devDependencies": {
    "commander": "^10.0.0",
    "esbuild": "^0.17.7",
    "tsconfig": "*",
    "tsup": "^6.6.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@currents/cypress-debugger-plugin": "*",
    "@currents/cypress-debugger-support": "*"
  },
  "peerDependencies": {
    "cypress": ">=10.0.0"
  },
  "tsup": {
    "entry": [
      "./index.ts"
    ],
    "external": [
      "cypress"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "shims": true,
    "clean": true,
    "sourcemap": "inline",
    "platform": "node",
    "target": "es2018"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  }
}
