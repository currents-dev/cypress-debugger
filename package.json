{
  "name": "cypress-debugger",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --check .",
    "format-fix": "prettier --write .",
    "prepare": "husky install",
    "publish-npm": "turbo run publish-npm",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@jest/globals": "^29.6.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.0",
    "jest": "^29.6.4",
    "postcss": "^8.4.22",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.6.0"
}
